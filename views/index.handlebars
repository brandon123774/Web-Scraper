<header class="header">
  <div class="col-md-12 jumbotron">
    <div class="text-center">
      <h1>Scraper App</h1>
    </div>
  </div>

  <nav class="button-group">
    <button class="button-dark" id="scrape">Scrape Article</button>
     <button class="button-dark" id="savedarticles">Saved Article</button>
  </nav>
</header>

<div class="filler"></div>

<div>

  {{#each items}}
    {{#unless saved}}

      <p>{{this.title}}</p>
      <p>{{this.link}}</p>
      <button class="button-dark save" data-id={{this.id}}>Save Article</button>

  {{/unless}}
  {{/each}}

</div>
